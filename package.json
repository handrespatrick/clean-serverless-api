{
  "name": "clean-serverless-api",
  "version": "1.0.0",
  "description": "Aplicação serverless dedicada a listar as informações do personagem do Star Wars, passando o nome como parametro.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write ./src",
    "lint-staged-husky": "lint-staged",
    "commit": "cz",
    "build": "./scripts.sh build",
    "prepare": "husky install",
    "patch": "npm version patch -m \"release: version %s\" && git push --tags && git push",
    "test": "chmod u+x scripts.sh ; ./scripts.sh test",
    "start": "chmod u+x scripts.sh ; ./scripts.sh start",
    "start:event": "chmod u+x scripts.sh ; ./scripts.sh event defaultFunction",
    "clean": "shx rm -rf coverage dist build && shx rm -rf node_modules",
    "release": "release-it",
    "update-readme": "scafflater-cli partial:run update-readme",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.6.0",
    "@commitlint/types": "^18.4.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.2.1",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "^3.0.1",
    "pretty-quick": "3.1.3",
    "release-it": "^16.1.3",
    "serverless": "^3.33.0",
    "serverless-api-gateway-caching": "^1.10.3",
    "serverless-domain-manager": "^7.1.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-conditional-functions": "^1.0.8",
    "serverless-plugin-lambda-insights": "^2.0.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-tscpaths": "^0.0.8",
    "shx": "0.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  }
}
